<IfModule LiteSpeed>
    RewriteEngine on
    RewriteCond %{HTTP_HOST} ^(www.)?foxyloxy.co.uk$
    RewriteCond %{REQUEST_URI} !^/wordpress/
    RewriteCond %{REQUEST_FILENAME} !-f
    RewriteCond %{REQUEST_FILENAME} !-d
    RewriteRule ^(.*)$ /wordpress/$1
    RewriteCond %{HTTP_HOST} ^(www.)?foxyloxy.co.uk$
    RewriteRule ^(/)?$ wordpress/index.php [L]
</IfModule>